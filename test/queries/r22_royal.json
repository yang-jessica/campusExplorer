{
    "title": "Apply: MAX should be supported.",
    "query": {
        "WHERE": {
            "AND": [
                {
                    "IS": {
                        "rooms_furniture": "*Tables*"
                    }
                },
                {
                    "GT": {
                        "rooms_seats": 300
                    }
                }
            ]
        },
        "OPTIONS": {
            "COLUMNS": [
                "rooms_shortname",
                "maxSeats"
            ],
            "ORDER": {
                "dir": "DOWN",
                "keys": [
                    "maxSeats"
                ]
            }
        },
        "TRANSFORMATIONS": {
            "GROUP": [
                "rooms_shortname"
            ],
            "APPLY": [
                {
                    "maxSeats": {
                        "MAX": "rooms_seats"
                    }
                }
            ]
        }
    },
    "response": {
        "code": 200,
        "body": {
            "result": [
                {
                    "rooms_shortname": "OSBO",
                    "maxSeats": 442
                },
